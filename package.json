    "@types/node": "^20.12.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.56.0",
    "prettier": "^3.3.2",
    "prisma": "^5.17.0",
    "tsx": "^4.20.5",
    "typescript": "^5.4.5"
